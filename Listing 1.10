CREATE PROCEDURE My_Demo_Proc (OUT o_param INT) 
LANGUAGE SQLSCRIPT 
READS SQL DATA AS
BEGIN
	DECLARE V_1 int;
	DECLARE V_2 int = 0;
	DECLARE V_3 int DEFAULT 0;
      
       t = select * from SALES ;
       select count(*) into V_1 from :t;
       V_2 = :V_1 + 1;
       o_param = :V_2 + :V_3;
END;
